clearfix()
  zoom: 1

  &::before
  &::after
    content: ''
    display: block

  &::after
    clear: both

nolist()
  list-style: none outside

sprite($sprite)
  if !match('hover', selector()) && !match('active', selector())
    spriteImage($sprite)
  spritePosition($sprite)
  spriteWidth($sprite)
  spriteHeight($sprite)
